package com.arun.sample;
import java.util.*;
public class WAP_11 {
	int s=0,r,a;
	public int add(int m)
	{
		while(k>0)
		{
			r=m%10;
			s+=r;
			m=m/10;
		}
		return s;
	}
	public static void main(String []args)
	{
		WAP_11 o=new WAP_11();
	Scanner sc=new Scanner(System.in);
	System.out.println("enter a num");
	int k=sc.nextInt();
	a=k;
	while(k>=10)
	{
		a=o.add(k);
		k=a;
	}
	if (k==1)
		System.out.println("magic "+k);
	else
		System.out.println("not "+k);
	}
}
